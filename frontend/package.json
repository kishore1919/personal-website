{
    "proxy": "http://localhost:8080",
    "name": "my-first-web",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "craco start",
        "build-dev": "rm -rf build && craco build",
        "build": "yarn build-dev && yarn cleanup",
        "clean": "rm -rf node_modules && rm -rf yarn.lock && yarn",
        "remove-devDep": "yarn remove @types/react @types/react-dom @types/react-router-dom @types/styled-components typescript @craco/craco craco-esbuild react-scripts",
        "cleanup": "echo cleaning up frontend && concurrently \"rm -rf src\" \"yarn remove-devDep\""
    },
    "dependencies": {
        "@craco/craco": "^6.4.3",
        "@types/react": "^17.0.37",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.3.2",
        "@types/styled-components": "^5.1.15",
        "craco-esbuild": "^0.4.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet-async": "^1.1.2",
        "react-icons": "^4.2.0",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^5.0.0",
        "react-spinners": "^0.11.0",
        "styled-components": "^5.3.1",
        "typescript": "^4.1.2",
        "web-vitals": "^1.0.1",
        "workbox-background-sync": "^5.1.3",
        "workbox-broadcast-update": "^5.1.3",
        "workbox-cacheable-response": "^5.1.3",
        "workbox-core": "^5.1.3",
        "workbox-expiration": "^5.1.3",
        "workbox-google-analytics": "^5.1.3",
        "workbox-navigation-preload": "^5.1.3",
        "workbox-precaching": "^5.1.3",
        "workbox-range-requests": "^5.1.3",
        "workbox-routing": "^5.1.3",
        "workbox-strategies": "^5.1.3",
        "workbox-streams": "^5.1.3"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
